}

@book{cohen1995self,
  author = {Cohen, G. A.},
  title = {Self-Ownership, Freedom, and Equality},
  publisher = {Cambridge University Press},
  address = {Cambridge},
  year = {1995},
  note = {CORE ARGUMENT: Critiques libertarian self-ownership thesis and its implications for property rights. Argues that self-ownership cannot deliver the freedom it promises because freedom requires access to external resources, not just ownership of one's body/labor. Even with self-ownership, lack of access to means of production leaves workers unfree and exploited. Shows that Marxist exploitation critique actually presupposes self-ownership, creating tension in Marxist theory. Argues for egalitarian distribution of external resources to make self-ownership meaningful. Self-ownership plus unequal resource distribution yields unfreedom for the poor. RELEVANCE: Provides left-wing critique of property theory and shows how formal property rights can be inadequate for real freedom. Relevant to debates about whether housing security requires actual property rights or merely access. Challenges assumption that property rights alone guarantee freedom. Important for understanding how unequal property distribution constrains freedom. Informs arguments for housing redistribution and tenant protections. POSITION: Egalitarian critique of libertarian property - self-ownership requires egalitarian resource distribution.},
  keywords = {Cohen, self-ownership, egalitarianism, property-critique, Medium}
}

