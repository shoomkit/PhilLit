}

@article{harding2019spoilage,
  author = {Harding, Eloise},
  title = {Spoilage and Squatting: {A} {L}ockean Argument},
  journal = {Res Publica},
  year = {2019},
  volume = {26},
  number = {3},
  pages = {299--317},
  doi = {10.1007/s11158-019-09445-0},
  note = {CORE ARGUMENT: Defends certain forms of squatting using Locke's spoilage proviso. Locke argued that one must not appropriate more than one can use without spoilage; this proviso limits both initial appropriation and continued retention of property. When legal owners let buildings fall into dereliction/abandonment, they violate the spoilage proviso and lose their legitimate property claim. Squatters who occupy abandoned buildings and put them to productive use have superior claim based on Lockean labor theory. Uses case studies where squatters reversed building dereliction. Argues for occupation-based property rights over formal title when owners fail to use property. RELEVANCE: Provides philosophical justification for adverse possession and squatting in housing contexts. Relevant to debates about vacant/abandoned properties and whether occupancy/use can trump formal ownership. Shows how classical property theory (Locke) can ground occupancy-based claims. Important for understanding limits of ownership and when use-rights might supersede title. Connects to housing justice arguments about vacant property. POSITION: Lockean labor/spoilage theory applied to justify squatting in abandoned buildings.},
  keywords = {squatting, Locke, spoilage-proviso, adverse-possession, use-rights, Medium}
}

@book{locke1988treatises,
  author = {Locke, John},
  title = {Two Treatises of Government},
  editor = {Laslett, Peter},
  publisher = {Cambridge University Press},
  address = {Cambridge},
  year = {1988},
  note = {Original work published 1689. CORE ARGUMENT: Develops influential labor theory of property: God gave the earth to mankind in common, but individuals can appropriate unowned resources by mixing their labor with them. Labor creates property rights because each person owns their own body and thus owns their labor. Property appropriation is legitimate when: (1) enough and as good is left for others (sufficiency proviso), (2) one doesn't appropriate more than one can use without spoilage (spoilage proviso). Property precedes government and exists in state of nature. Government's role is to protect pre-existing property rights. RELEVANCE: Foundational text for labor/desert theories of property. Provisos (sufficiency, spoilage) provide resources for limiting property claims and justifying redistribution. Relevant to debates about whether occupancy/use should be required for continued property rights (spoilage proviso). Classical reference point for all subsequent property theory. Labor theory provides alternative to pure occupancy theory. POSITION: Labor theory of property with provisos limiting appropriation.},
  keywords = {Locke, labor-theory, property-foundations, classical, foundational, High}
}

@book{hegel1967philosophy,
