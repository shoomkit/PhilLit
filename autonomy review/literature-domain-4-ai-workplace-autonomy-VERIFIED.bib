@article{kellogg2020algorithms,
  author = {Kellogg, Katherine C. and Valentine, Melissa A. and Christin, Ang{\`e}le},
  title = {Algorithms at Work: The New Contested Terrain of Control},
  journal = {Academy of Management Annals},
  year = {2020},
  volume = {14},
  number = {1},
  pages = {366--410},
  doi = {10.5465/annals.2018.0174},
  note = {CORE ARGUMENT: Develops foundational framework for understanding algorithmic control in workplaces through six mechanisms (the "6 Rs"): algorithms direct workers by restricting and recommending, evaluate workers by recording and rating, and discipline workers by replacing and rewarding. Draws on Edwards' contested terrain theory to analyze how algorithms reshape organizational control and worker resistance. Systematic review synthesizes diverse literatures on algorithmic management across sectors. RELEVANCE: Provides the dominant theoretical framework for understanding algorithmic workplace control. The 6 Rs framework is essential for any analysis of how AI systems affect worker autonomy. Directly addresses control-autonomy tension central to philosophical debates about agency and freedom in technologically mediated work. Finalist for 2021 Best Article Award from Academy of Management. POSITION: Foundational algorithmic management framework (organizational theory perspective).},
  keywords = {algorithmic-management, workplace-control, theoretical-framework, High}
}

@article{jarrahi2021algorithmic,
  author = {Jarrahi, Mohammad Hossein and Newlands, Gemma and Lee, Min Kyung and Wolf, Christine T. and Kinder, Eliscia and Sutherland, Will},
  title = {Algorithmic management in a work context},
  journal = {Big Data \& Society},
  year = {2021},
  volume = {8},
  number = {2},
  doi = {10.1177/20539517211020332},
  note = {CORE ARGUMENT: Takes sociotechnical approach to algorithmic management, arguing it reflects both technological infrastructures and organizational choices. Identifies three key issues: how algorithmic management shapes power dynamics between workers and managers, how it demands new roles and competencies while fostering oppositional attitudes, and how it impacts knowledge exchange through technical and organizational opacity. Emphasizes that algorithmic management is spreading from platform-mediated gig economy to standard work settings. RELEVANCE: Important synthesis that bridges platform work and traditional employment contexts. Highlights that algorithmic management is not just a gig economy phenomenon but transforming conventional workplaces. The focus on opacity and information asymmetries directly relates to debates about transparency requirements for autonomous decision-making. POSITION: Sociotechnical algorithmic management framework (HCI/organizational perspective).},
  keywords = {algorithmic-management, sociotechnical-systems, opacity, power-dynamics, High}
}

@article{wood2019good,
  author = {Wood, Alex J. and Graham, Mark and Lehdonvirta, Vili and Hjorth, Isis},
  title = {Good Gig, Bad Gig: Autonomy and Algorithmic Control in the Global Gig Economy},
  journal = {Work, Employment and Society},
  year = {2019},
  volume = {33},
  number = {1},
  pages = {56--75},
  doi = {10.1177/0950017018785616},
  note = {CORE ARGUMENT: Evaluates job quality in remote gig economy through mixed methods (107 interviews in six countries, N=679 survey). Finds that algorithmic management offers high flexibility and autonomy but also low pay, social isolation, irregular hours, and exhaustion. Contrasts with accounts suggesting ICTs lead to Taylorism, finding that non-proximity of workers and clients limits direct supervision, creating perceived autonomy despite algorithmic constraints. Reveals significant global variation in platform work conditions. RELEVANCE: Essential empirical study showing complexity of autonomy in platform work - workers experience both freedom (schedule flexibility) and constraint (algorithmic control) simultaneously. Demonstrates that autonomy is multidimensional and context-dependent, challenging simplistic narratives. Global scope reveals how algorithmic management operates differently across regions and labor markets. POSITION: Empirical platform work research (mixed methods, global comparative).},
  keywords = {platform-work, gig-economy, autonomy, algorithmic-control, empirical-study, High}
}

@article{vallas2020what,
  author = {Vallas, Steven and Schor, Juliet B.},
  title = {What Do Platforms Do? Understanding the Gig Economy},
  journal = {Annual Review of Sociology},
  year = {2020},
  volume = {46},
  pages = {273--294},
  doi = {10.1146/annurev-soc-121919-054857},
  note = {CORE ARGUMENT: Synthesizes literature on platform economy using four metaphors: platforms as entrepreneurial incubators, digital cages, accelerants of precarity, and chameleons. Proposes alternative conception of platforms as "permissive potentates" that externalize responsibility while exercising concentrated power. Argues platforms represent distinct governance mechanism different from markets, hierarchies, or networks. One of top ten most-read Annual Review pieces of 2020. RELEVANCE: Comprehensive theoretical synthesis essential for understanding how platforms reshape labor relations and power structures. The "permissive potentates" concept captures paradox of platform control - workers appear independent but face intense algorithmic management. Directly addresses questions of autonomy, control, and responsibility distribution relevant to philosophical analysis of agency. POSITION: Critical platform economy synthesis (sociological perspective).},
  keywords = {platform-economy, governance, gig-work, theoretical-synthesis, High}
}

@inproceedings{lee2015working,
  author = {Lee, Min Kyung and Kusbit, Daniel and Metsky, Evan and Dabbish, Laura},
  title = {Working with Machines: The Impact of Algorithmic and Data-Driven Management on Human Workers},
  booktitle = {Proceedings of the 33rd Annual ACM Conference on Human Factors in Computing Systems (CHI 2015)},
  year = {2015},
  pages = {1603--1612},
  doi = {10.1145/2702123.2702548},
  note = {CORE ARGUMENT: Foundational HCI paper that coined term "algorithmic management" to describe managerial role played by algorithms on Uber and Lyft platforms. Defines algorithmic management as "software algorithms that assume managerial functions and surrounding institutional devices that support algorithms in practice." Explores how drivers respond when algorithms assign work, provide informational support, and evaluate performance. Identifies information asymmetries and lack of transparency as core issues. RELEVANCE: Seminal paper establishing algorithmic management as HCI research area. First systematic examination of how algorithmic systems function as managers, not just tools. Information asymmetry findings crucial for understanding barriers to worker autonomy - workers cannot make informed decisions without understanding how algorithms work. Highly influential (529 citations, 8,980 downloads). POSITION: Foundational HCI algorithmic management research (empirical, Uber/Lyft focus).},
  keywords = {algorithmic-management, HCI, rideshare, information-asymmetry, High}
}

@book{rosenblat2018uberland,
  author = {Rosenblat, Alex},
  title = {Uberland: How Algorithms Are Rewriting the Rules of Work},
  publisher = {University of California Press},
  year = {2018},
  address = {Oakland, CA},
  note = {CORE ARGUMENT: Based on ethnographic fieldwork (5,000+ miles riding with Uber drivers, four years across 25+ cities), examines how Uber uses information asymmetries, surveillance through customer ratings, and behavioral nudges to manage drivers. Argues platforms present as marketplaces but act as traditional employers, using algorithmic management to produce what observers define as "managed labor force." Chronicles drivers' experiences with unpredictability, gamification, and manipulation through algorithmic wage discrimination and surge pricing. RELEVANCE: Comprehensive ethnographic account of algorithmic management in archetypal platform. Information asymmetry and behavioral nudging directly bear on questions of worker autonomy and informed decision-making. Shows how algorithmic opacity undermines conditions for autonomous agency. Notable that author now works as head of marketplace policy at Uber, providing insider perspective on platform design choices. POSITION: Critical ethnography of Uber (platform capitalism perspective).},
  keywords = {uber, algorithmic-management, ethnography, platform-work, High}
}

@book{gray2019ghost,
  author = {Gray, Mary L. and Suri, Siddharth},
  title = {Ghost Work: How to Stop Silicon Valley from Building a New Global Underclass},
  publisher = {Houghton Mifflin Harcourt},
  year = {2019},
  address = {Boston, MA},
  note = {CORE ARGUMENT: Five-year study of crowdwork and Amazon Mechanical Turk unveils invisible human labor force that makes AI and automation appear to work. "Ghost workers" perform high-tech piecework (content moderation, data labeling, proofreading) that is devalued, unacknowledged, and excluded from labor protections. Estimates 8% of Americans work in this "ghost economy" with below-legal minimum wages, no benefits, and arbitrary termination. Combines ethnography, interviews, surveys, and large-scale platform data analysis. RELEVANCE: Reveals hidden labor essential for AI systems, challenging narratives of full automation. Ghost workers' invisibility and precarity raise fundamental questions about dignity, recognition, and moral standing in AI-mediated work. Lack of labor protections demonstrates how platform architectures can systematically undermine autonomy and security. Essential for understanding political economy of AI development. POSITION: Critical crowdwork ethnography (anthropology + computer science collaboration).},
  keywords = {crowdwork, mechanical-turk, invisible-labor, precarity, High}
}

@article{griesbach2019algorithmic,
  author = {Griesbach, Kathleen and Reich, Adam and Elliott-Negri, Luke and Milkman, Ruth},
  title = {Algorithmic Control in Platform Food Delivery Work},
  journal = {Socius: Sociological Research for a Dynamic World},
  year = {2019},
  volume = {5},
  doi = {10.1177/2378023119870041},
  note = {CORE ARGUMENT: Analyzes food delivery platforms through 55 interviews and 955-worker survey, examining variation in algorithmic control intensity. Finds Instacart exerts "algorithmic despotism" - stringent regulation of time and activities - while other platforms allow more discretion. Uses information asymmetry, customer ratings surveillance, and behavioral nudges as control mechanisms. Demonstrates that even within platform economy, significant variation exists in how tightly algorithms constrain worker autonomy and discretion. RELEVANCE: Important empirical demonstration that algorithmic control is not monolithic - platform design choices create different autonomy conditions. "Algorithmic despotism" concept captures most constraining form of algorithmic management. Variation in control intensity shows autonomy is design choice, not technological inevitability, with implications for regulation and alternative platform models. POSITION: Empirical platform work research (food delivery focus, comparative).},
  keywords = {food-delivery, algorithmic-control, platform-work, empirical-study, Medium}
}

@article{salehi2015dynamo,
  author = {Salehi, Niloufar and Irani, Lilly C. and Bernstein, Michael S.},
  title = {We Are Dynamo: Overcoming Stalling and Friction in Collective Action for Crowd Workers},
  booktitle = {Proceedings of the 33rd Annual ACM Conference on Human Factors in Computing Systems (CHI 2015)},
  year = {2015},
  pages = {1621--1630},
  doi = {10.1145/2702123.2702593},
  note = {CORE ARGUMENT: Presents first socio-technical system supporting collective action for online workers. Dynamo platform enabled Amazon Mechanical Turk workers to collaboratively author ethical research guidelines and organize letter-writing campaigns. Demonstrates that platform workers can organize despite geographic dispersion and platform design that individualizes labor. Workers used Dynamo to humanize themselves in public imagination and advocate for better treatment. Awarded honorable mention at CHI 2015. RELEVANCE: Proves that worker agency and collective action are possible even under atomizing platform architectures. Challenges deterministic views of algorithmic control by showing workers developing tools for solidarity and advocacy. Demonstrates that technology can support rather than undermine worker autonomy when designed for collective empowerment. Socio-technical intervention shows alternative possibilities for platform design. POSITION: HCI worker empowerment research (design intervention, collective action).},
  keywords = {collective-action, crowdwork, worker-organizing, HCI, sociotechnical-design, High}
}

@inproceedings{irani2013turkopticon,
  author = {Irani, Lilly and Silberman, M. Six},
  title = {Turkopticon: Interrupting Worker Invisibility in Amazon Mechanical Turk},
  booktitle = {Proceedings of the SIGCHI Conference on Human Factors in Computing Systems (CHI 2013)},
  year = {2013},
  pages = {611--620},
  doi = {10.1145/2470654.2470742},
  note = {CORE ARGUMENT: Analyzes Amazon Mechanical Turk as site of technically mediated worker-employer relations where human computation relies on worker invisibility. Presents Turkopticon, activist browser extension that allows workers to publicize and evaluate relationships with employers by inserting reviews into AMT's task marketplace interface. Enables workers to share job information, hold employers accountable, and make visible the power asymmetries built into platform architecture. Best Paper award at CHI 2013. RELEVANCE: Seminal work on worker invisibility and information asymmetries in crowdwork platforms. Turkopticon demonstrates worker-created tools for transparency and accountability as resistance to algorithmic opacity. Visibility/invisibility directly relates to recognition and moral standing - prerequisites for autonomy. Shows workers actively reshaping information environment despite platform constraints. Foundational for understanding worker agency in platform contexts. POSITION: Critical HCI research (worker advocacy, activist system design).},
  keywords = {turkopticon, mechanical-turk, worker-invisibility, activism, transparency, High}
}

@report{oecd2025algorithmic,
  author = {{OECD}},
  title = {Algorithmic Management in the Workplace: New Evidence from an OECD Employer Survey},
  institution = {Organisation for Economic Co-operation and Development},
  year = {2025},
  address = {Paris},
  url = {https://www.oecd.org/en/publications/algorithmic-management-in-the-workplace_287c13c4-en.html},
  note = {CORE ARGUMENT: First large-scale employer survey on algorithmic management across six countries (France, Germany, Italy, Japan, Spain, United States). Surveyed 6,047 mid-level managers in 2023-2024, finding 74% of firms use software to automate managerial tasks (instructing, monitoring, evaluating). Adoption reaches 90% in US, 79% average in Europe. Managers perceive improved decision quality but also trustworthiness concerns: unclear accountability, inability to follow logic, inadequate worker health protection. Worker surveys show reduced job satisfaction, increased workloads, stress, and job insecurity. RELEVANCE: Most comprehensive institutional data on algorithmic management adoption and impacts. Demonstrates algorithmic management is mainstream in developed economies, not fringe phenomenon. Trustworthiness concerns from managers themselves highlight governance gaps. Worker well-being data crucial for assessing autonomy costs. Essential empirical foundation for normative analysis of algorithmic workplace control. POSITION: Institutional policy research (empirical, cross-national survey data).},
  keywords = {algorithmic-management, OECD, institutional-report, empirical-data, well-being, High}
}

@report{ilo2023generative,
  author = {Gmyrek, Pawel and Berg, Janine and Bescond, David},
  title = {Generative AI and Jobs: A Global Analysis of Potential Effects on Job Quantity and Quality},
  institution = {International Labour Office},
  year = {2023},
  address = {Geneva},
  number = {ILO Working Paper 96},
  url = {https://www.ilo.org/publications/generative-ai-and-jobs-global-analysis-potential-effects-job-quantity-and-quality},
  note = {CORE ARGUMENT: First ILO analysis of generative AI's potential effects on global employment. Finds that augmentation more likely than automation for most occupations, but significant variation by gender and region. Women more vulnerable to automating effects, particularly in clerical and business process outsourcing roles prevalent in developing economies. Contrasts with earlier automation studies by examining AI's capacity to augment rather than fully replace work tasks. Introduces task-based methodology for assessing AI exposure across global labor markets. RELEVANCE: Shifts automation debate from wholesale job displacement to more nuanced task-level analysis. Gender and geographic disparities in AI exposure raise equity concerns central to normative evaluation. Augmentation vs. automation distinction important for understanding how AI reshapes rather than eliminates work. Foundation for subsequent ILO-NASK collaboration on detailed occupational exposure indices. POSITION: Institutional labor market analysis (task-based framework, global scope).},
  keywords = {generative-AI, automation, ILO, labor-market-impacts, gender, Medium}
}

@report{ilo2024algorithmic,
  author = {{ILO}},
  title = {Algorithmic Management Practices in Regular Workplaces: Empirical Evidence from Four Countries},
  institution = {International Labour Organization},
  year = {2024},
  address = {Geneva},
  url = {https://www.ilo.org/media/515561/download},
  note = {CORE ARGUMENT: Examines algorithmic management expansion into regular workplaces beyond gig economy through case studies in Italy, France, India, and South Africa. Finds limited empirical evidence available, relies on workplace case studies to examine technology's impact on labor processes. Demonstrates algorithmic management is not confined to platform work but spreading across traditional employment relationships, albeit with limited documentation. Cross-national comparison reveals how institutional contexts shape algorithmic management adoption and worker experiences. RELEVANCE: Documents spread of algorithmic management from platforms to conventional workplaces, challenging view that algorithmic control is gig economy-specific phenomenon. Case study methodology provides rich detail on how algorithmic systems reshape traditional employment. Global South inclusion essential for understanding algorithmic management as global not just Western phenomenon. Highlights need for more empirical research in this area. POSITION: Institutional empirical research (case study methodology, global comparative).},
  keywords = {algorithmic-management, ILO, traditional-workplaces, global-south, case-studies, Medium}
}

@report{berg2018digital,
  author = {Berg, Janine and Furrer, Marianne and Harmon, Ellie and Rani, Uma and Silberman, M. Six},
  title = {Digital Labour Platforms and the Future of Work: Towards Decent Work in the Online World},
  institution = {International Labour Office},
  year = {2018},
  address = {Geneva},
  isbn = {9789220310243},
  url = {https://www.ilo.org/publications/digital-labour-platforms-and-future-work-towards-decent-work-online-world},
  note = {CORE ARGUMENT: First major ILO comparative study of working conditions on web-based crowdwork platforms. Survey of 3,500 workers in 75 countries across five major microtask platforms analyzes pay rates, work availability, intensity, rejections/non-payment, worker-client communication, social protection, work-life balance, and career prospects. Finds significant decent work deficits: low pay, unpredictable income, no social protection, high rejection rates. Establishes empirical baseline for understanding global crowdwork conditions. RELEVANCE: Comprehensive empirical documentation of crowdwork conditions globally. Demonstrates how platform architectures systematically undermine decent work standards and worker security. International scope reveals common patterns across platforms despite geographic dispersion. Foundational for understanding precarity and autonomy constraints in online labor markets. Essential for connecting philosophical concepts of autonomy to lived realities of platform workers. POSITION: Institutional empirical research (large-scale survey, decent work framework).},
  keywords = {crowdwork, platform-labor, ILO, decent-work, global-survey, High}
}

@article{acemoglu2020robots,
  author = {Acemoglu, Daron and Restrepo, Pascual},
  title = {Robots and Jobs: Evidence from US Labor Markets},
  journal = {Journal of Political Economy},
  year = {2020},
  volume = {128},
  number = {6},
  pages = {2188--2244},
  doi = {10.1086/705716},
  note = {CORE ARGUMENT: Analyzes effect of industrial robot adoption (1990-2007) on US labor markets. Develops theoretical framework where automation enables capital to replace labor in tasks (displacement effect) while new task creation reinstates labor (reinstatement effect). Empirically estimates 420,000-720,000 jobs lost to automation in US manufacturing 1990-2014. Finds displacement dominates: automation reduces labor share and may reduce labor demand even while raising productivity. Foundational for task-based approach to automation analysis. RELEVANCE: Influential economic framework for understanding automation's labor market effects. Displacement vs. reinstatement tension parallels autonomy debates - technology can both substitute for and complement human agency. Manufacturing focus shows automation impacts extend beyond white-collar work. Essential for connecting AI/automation discussions to empirical labor market outcomes. Challenges optimistic narratives about automation automatically creating equivalent new opportunities. POSITION: Economic analysis of automation (task-based framework, empirical labor markets).},
  keywords = {automation, robots, labor-displacement, task-based-approach, economics, Medium}
}

@article{acemoglu2019automation,
  author = {Acemoglu, Daron and Restrepo, Pascual},
  title = {Automation and New Tasks: How Technology Displaces and Reinstates Labor},
  journal = {Journal of Economic Perspectives},
  year = {2019},
  volume = {33},
  number = {2},
  pages = {3--30},
  doi = {10.1257/jep.33.2.3},
  note = {CORE ARGUMENT: Develops theoretical framework for how automation affects labor demand through task reallocation. Automation shifts task content of production against labor through displacement effect. Counterbalancing force is creation of new tasks where labor has comparative advantage, generating reinstatement effect that raises labor share and demand. Framework allows analysis of how different technologies (automation vs. new tasks) have differential impacts on labor. Historical analysis shows technology can both substitute for and complement workers. RELEVANCE: Theoretical foundation for understanding how AI systems reshape work through task reorganization. Displacement-reinstatement framework useful for analyzing autonomy effects - some AI systems substitute for human judgment (reducing autonomy) while others create new domains for human agency (expanding autonomy). Essential for moving beyond simplistic automation/augmentation binary to more nuanced task-level analysis. POSITION: Economic theory of automation (task-based framework, historical perspective).},
  keywords = {automation, task-theory, labor-economics, displacement, Medium}
}

@book{brynjolfsson2014second,
  author = {Brynjolfsson, Erik and McAfee, Andrew},
  title = {The Second Machine Age: Work, Progress, and Prosperity in a Time of Brilliant Technologies},
  publisher = {W. W. Norton \& Company},
  year = {2014},
  address = {New York},
  note = {CORE ARGUMENT: Argues we are entering Second Machine Age where automation overtakes cognitive requirements of labor, rendering humans largely unnecessary for many tasks. Unlike First Machine Age (physical power), current era involves automation of cognitive tasks, making humans and software substitutes rather than complements. Predicts profound labor market disruption: fewer people working, falling wages even as productivity and profits soar. Professions from lawyers to truck drivers will be upended. Companies must transform or die. Influential framing of AI/automation as fundamentally transformative for work. RELEVANCE: Influential popularization of AI automation concerns. Second Machine Age framing shapes how policymakers and public understand AI's workplace impacts. Substitution vs. complementarity distinction crucial for autonomy debates - if AI substitutes for human judgment, what role remains for human agency? Optimistic about technological progress but acknowledges serious labor market challenges. Essential for understanding mainstream narratives about AI and work. POSITION: Technology optimist perspective (economic analysis, popular audience).},
  keywords = {automation, second-machine-age, AI-impacts, future-of-work, Medium}
}

@article{frey2017future,
  author = {Frey, Carl Benedikt and Osborne, Michael A.},
  title = {The Future of Employment: How Susceptible Are Jobs to Computerisation?},
  journal = {Technological Forecasting and Social Change},
  year = {2017},
  volume = {114},
  pages = {254--280},
  doi = {10.1016/j.techfore.2016.08.019},
  note = {CORE ARGUMENT: Implements novel Gaussian process classifier methodology to estimate computerization probability for 702 detailed occupations. Based on 2013 expert workshop with 70 machine learning researchers who labeled 70 jobs. Estimates that half of US employment susceptible to automation within 20 years. Highly influential (spawned numerous follow-up studies globally) but also controversial - later research questions methodological choices and prediction accuracy. Focuses on technological feasibility, not economic, legal, or social barriers to automation. RELEVANCE: Most widely-cited automation risk study, shaping policy debates despite methodological critiques. Demonstrates how expert judgment about technological capabilities translates into employment forecasts. Raises fundamental questions about scale of potential work transformation and need for policy responses. Technological feasibility vs. actual adoption distinction important for understanding automation trajectories. Essential reference point despite limitations. POSITION: Automation risk forecasting (expert elicitation methodology, technology-centric).},
  keywords = {automation, job-displacement, forecasting, computerization, Medium}
}

@book{kalleberg2018precarious,
  author = {Kalleberg, Arne L.},
  title = {Precarious Lives: Job Insecurity and Well-Being in Rich Democracies},
  publisher = {Polity Press},
  year = {2018},
  address = {Cambridge, UK},
  note = {CORE ARGUMENT: Analyzes how precarious employment extends beyond job characteristics to shape workers' entire "precarious lives" - affecting health, family formation, political participation, and civic engagement. Documents rise of job insecurity in rich democracies since 1970s through erosion of employment protections, spread of nonstandard work arrangements, and increased employer power. Examines macro-level economic and sociological shifts underpinning precarity: financialization, globalization, declining union power, policy changes. Well-being impacts demonstrate that employment precarity has broader social consequences. RELEVANCE: Comprehensive analysis of precarity as defining feature of contemporary employment. Platform work and algorithmic management must be understood within broader context of declining job security. Precarity undermines autonomy by creating dependency and vulnerability - workers lack security to resist unfavorable conditions. Well-being effects demonstrate that job quality affects human flourishing, not just economic outcomes. Essential for understanding structural conditions shaping worker agency. POSITION: Sociological analysis of precarity (macro-level, comparative democracies).},
  keywords = {precarity, job-insecurity, well-being, labor-markets, Medium}
}

@book{srnicek2017platform,
  author = {Srnicek, Nick},
  title = {Platform Capitalism},
  publisher = {Polity Press},
  year = {2017},
  address = {Cambridge, UK},
  note = {CORE ARGUMENT: Traces historical roots of platform capitalism to 1970s downturn, positioning platforms as response to declining profit rates in traditional industries. Defines platforms as digital infrastructures enabling interaction between multiple user groups. Identifies five platform types: advertising (Google, Facebook), cloud (Salesforce), industrial (GE), product (Spotify), and lean (Uber, Airbnb). Data is lifeblood of platform capitalism - platforms accumulate behavioral data to optimize engagement, target ads, and develop machine learning. Platforms represent distinct business model and governance mechanism. RELEVANCE: Influential political economy framework for understanding platforms beyond technological features. Platform capitalism connects contemporary digital platforms to longer capitalist history. Data extraction and accumulation central to how platforms exercise control and generate value. Lean platforms (Uber, Airbnb) minimize assets while maximizing data collection and algorithmic control. Essential for critical analysis situating algorithmic management within broader economic transformations. POSITION: Critical platform capitalism analysis (political economy, historical perspective).},
  keywords = {platform-capitalism, political-economy, data, business-models, High}
}

@book{zuboff2019surveillance,
  author = {Zuboff, Shoshana},
  title = {The Age of Surveillance Capitalism: The Fight for a Human Future at the New Frontier of Power},
  publisher = {PublicAffairs},
  year = {2019},
  address = {New York},
  note = {CORE ARGUMENT: Develops concept of "surveillance capitalism" as new form of capitalist accumulation that unilaterally claims human experience as free raw material for behavioral data extraction. Data are transformed into "behavioral surplus" through machine intelligence and fabricated into prediction products sold to business customers. Workplace applications include wearable technology for manager surveillance and behavioral control aimed at efficiency regardless of individual will. Argues this represents fundamentally new power asymmetry and threat to human autonomy and democracy. RELEVANCE: Influential critical framework connecting workplace surveillance to broader surveillance capitalism political economy. Behavioral data extraction and prediction markets undermine worker autonomy by treating workers as data sources rather than agents. Power asymmetries between platforms and users parallel those between employers and workers. Essential for understanding how algorithmic management fits within larger transformation of capitalism toward surveillance and behavioral modification. One of top non-fiction books of 2019. POSITION: Critical surveillance capitalism theory (political economy, democratic theory).},
  keywords = {surveillance-capitalism, data-extraction, workplace-surveillance, power, High}
}

@article{cameron2022expanding,
  author = {Cameron, Lindsey D. and Rahman, Hatim},
  title = {Expanding the Locus of Resistance: Understanding the Co-constitution of Control and Resistance in the Gig Economy},
  journal = {Organization Science},
  year = {2022},
  volume = {33},
  number = {1},
  pages = {38--58},
  doi = {10.1287/orsc.2021.1557},
  note = {CORE ARGUMENT: Three-year study of two platform companies (RideHail and FindWork) using participant observation, 189 interviews, and archival sources examines control-resistance relationship before, during, and after tasks. Argues platforms' use of algorithmically mediated customer control extends service encounter temporally and spatially, broadening span of both organizational control and workers' resistance. Demonstrates that resistance is not just reactive but co-constituted with control mechanisms. Workers resist through multiple tactics across expanded temporal and spatial domains. RELEVANCE: Important theoretical contribution showing control and resistance as mutually constitutive rather than oppositional. Expands understanding of where and when worker agency operates in platform contexts. Customer-mediated algorithmic control reveals how platforms distribute management functions across multiple actors. Demonstrates that platform architectures create new sites for both control and resistance. Essential for understanding worker agency as persistent despite algorithmic management. POSITION: Organizational ethnography (control-resistance dialectics, platform work).},
  keywords = {worker-resistance, platform-work, control, gig-economy, organizational-theory, Medium}
}

@article{cini2023resisting,
  author = {Cini, Lorenzo},
  title = {Resisting Algorithmic Control: Understanding the Rise and Variety of Platform Worker Mobilisations},
  journal = {New Technology, Work and Employment},
  year = {2023},
  volume = {38},
  number = {3},
  pages = {435--456},
  doi = {10.1111/ntwe.12257},
  note = {CORE ARGUMENT: Examines rise and variety of platform worker mobilizations through comparative analysis of crowdwork (Amazon Mechanical Turk) and work-on-demand (food delivery couriers). Workers resist algorithmic control through specific organizing modes, action repertoires, and collective solidarities. Challenges narrative that algorithmic control atomizes workers and prevents collective action. Documents global strikes, protests, and organizing efforts that have emerged to challenge platform-enabled algorithmic control. Shows workers demanding better pay, labor rights, and employment status recognition. RELEVANCE: Demonstrates that worker resistance and collective action persist despite algorithmic management designed to individualize labor. "Algoactivism" concept captures workers' emerging tactics for challenging algorithmic control. Essential for understanding worker agency as not just individual navigation but collective resistance. Organizing under algorithmic management requires new forms adapted to platform architectures. Challenges deterministic views of algorithmic control as totalizing. POSITION: Labor sociology (collective action, platform worker organizing, comparative).},
  keywords = {worker-resistance, collective-action, platform-work, labor-organizing, algoactivism, High}
}

@article{curchod2020working,
  author = {Curchod, Corentin and Patriotta, Gerardo and Cohen, Laurie and Neysen, Nicolas},
  title = {Working for an Algorithm: Power Asymmetries and Agency in Online Work Settings},
  journal = {Administrative Science Quarterly},
  year = {2020},
  volume = {65},
  number = {3},
  pages = {644--676},
  doi = {10.1177/0001839219867024},
  note = {CORE ARGUMENT: Draws on interviews with 77 high-performing eBay sellers in France and Belgium to investigate power asymmetries generated by customer evaluations in online work settings. Develops grounded theory capturing how algorithmic management creates new power dynamics where workers must navigate both platform algorithms and customer assessments. Workers develop agency through understanding and working with algorithmic systems despite information asymmetries and platform control. Shows online work settings generate novel forms of power asymmetry distinct from traditional employment. RELEVANCE: Important theoretical contribution on power asymmetries in algorithmic work settings. Customer-mediated evaluation systems distribute managerial control beyond traditional employer-employee dyad. Workers retain agency by developing platform knowledge and strategic behaviors despite structural constraints. Demonstrates that agency and constraint coexist in algorithmic management contexts. Influential for understanding how algorithmic systems reshape workplace power relations. POSITION: Organizational theory (grounded theory, power and agency, eBay platforms).},
  keywords = {algorithmic-management, power-asymmetry, agency, online-work, customer-evaluation, High}
}

@article{moehlmann2021algorithmic,
  author = {M{\"o}hlmann, Mareike and Zalmanson, Lior and Henfridsson, Ola and Gregory, Robert W.},
  title = {Algorithmic Management of Work on Online Labor Platforms: When Matching Meets Control},
  journal = {MIS Quarterly},
  year = {2021},
  volume = {45},
  number = {4},
  pages = {1999--2022},
  doi = {10.25300/MISQ/2021/15333},
  note = {CORE ARGUMENT: Examines algorithmic management along two dimensions: matching (connecting workers with tasks) and control (monitoring and regulating work). Qualitative study of Uber drivers' perceptions, supplemented by interviews with Uber executives and engineers, develops grounded theory of algorithmic management on online labor platforms. While matching function receives attention, platforms also use algorithms for tight monitoring and control. Finds drivers perceive both benefits (income opportunities) and constraints (surveillance, manipulation) from algorithmic systems. RELEVANCE: Important theoretical contribution distinguishing matching from control functions of algorithmic management. Dual nature of algorithms as both coordinative and controlling reflects broader tension in platform design. Uber driver perspectives reveal experienced power asymmetries and information gaps. Executive interviews show how platform design choices reflect strategic decisions about control intensity. Essential for understanding algorithmic management as multifaceted rather than monolithic. POSITION: Information systems research (grounded theory, Uber focus, dual-function framework).},
  keywords = {algorithmic-management, online-labor-platforms, uber, matching, control, High}
}

@inproceedings{moehlmann2017hands,
  author = {M{\"o}hlmann, Mareike and Zalmanson, Lior},
  title = {Hands on the Wheel: Navigating Algorithmic Management and Uber Drivers' Autonomy},
  booktitle = {Proceedings of the International Conference on Information Systems (ICIS 2017)},
  year = {2017},
  address = {Seoul, South Korea},
  note = {CORE ARGUMENT: Examines tension between algorithmic management and workers' autonomy on Uber platform. Low transparency identified as main cause of driver discontent. Analyzes emergent tension based on power asymmetry: drivers choose Uber for freedom and autonomy but experience constant algorithmic oversight, governance, and control. Empirical analysis reveals conflict between drivers' autonomy expectations and platform's control mechanisms. Foundational work later expanded into full MIS Quarterly article (2021) with additional co-authors. RELEVANCE: Early identification of autonomy-control tension central to platform work debates. Transparency issues directly relate to conditions for autonomous agency - workers cannot exercise meaningful autonomy without understanding systems governing their work. Power asymmetry between platform and drivers shapes all aspects of work experience. Essential for understanding autonomy as contested terrain in algorithmic management contexts. POSITION: Information systems research (case study, Uber drivers, autonomy-control tension).},
  keywords = {uber, algorithmic-management, autonomy, transparency, power-asymmetry, Medium}
}

@article{gandini2019labour,
  author = {Gandini, Alessandro},
  title = {Labour Process Theory and the Gig Economy},
  journal = {Human Relations},
  year = {2019},
  volume = {72},
  number = {6},
  pages = {1039--1056},
  doi = {10.1177/0018726718790002},
  note = {CORE ARGUMENT: Applies labor process theory to gig economy, arguing theory offers unique tools for understanding how labor power becomes commodity when supply and demand is platform-mediated. Examines notions of "point of production," emotional labor, and control in platform contexts. Feedback, ranking, and rating systems serve purposes of managerialization and monitoring. Connects contemporary platform work to historical labor process debates, particularly Braverman's deskilling thesis and Edwards' control strategies. RELEVANCE: Important theoretical contribution linking platform work to established labor process tradition. Shows algorithmic management as latest iteration of capitalist control strategies, not fundamentally new phenomenon. Rating and ranking systems as emotional labor and control mechanisms reveals how platforms extract value through worker self-management. Essential for historical and critical perspective on algorithmic management. Bridges platform work studies with broader labor sociology. POSITION: Labor process theory (historical materialist perspective, critical sociology).},
  keywords = {labor-process-theory, gig-economy, platform-work, control, critical-theory, High}
}

@article{aneesh2009global,
  author = {Aneesh, A.},
  title = {Global Labor: Algocratic Modes of Organization},
  journal = {Sociological Theory},
  year = {2009},
  volume = {27},
  number = {4},
  pages = {347--370},
  doi = {10.1111/j.1467-9558.2009.01352.x},
  note = {CORE ARGUMENT: Develops concept of "algocracy" as distinct organizational form alongside bureaucracy and markets. Algocracy is ruled by programming/algorithms rather than legal-rational authority (bureaucracy) or price (markets). Information technologies constrain human participation in decision-making through algorithmic governance. Analyzes labor integration through programming code as fundamentally different from traditional organizational forms. Foundational for understanding algorithmic governance as novel form of social ordering. RELEVANCE: Pioneering theoretical work (concept developed in 1997, published 2009) establishing algorithmic governance as distinct organizational principle. Algocracy concept captures how algorithms constitute authority and control mechanisms, not just tools. Essential for understanding algorithmic management as representing fundamental shift in organizational logic. Influential for subsequent scholarship on algorithmic governance by Danaher, Yeung, and others. Provides sociological foundation for analyzing algorithmic systems as governance structures. POSITION: Sociological theory (algocracy concept, organizational forms, global labor).},
  keywords = {algocracy, algorithmic-governance, organizational-theory, labor-migration, High}
}

@article{arrieta2018data,
  author = {Arrieta-Ibarra, Imanol and Goff, Leonard and Jim{\'e}nez-Hern{\'a}ndez, Diego and Lanier, Jaron and Weyl, E. Glen},
  title = {Should We Treat Data as Labor? Moving beyond `Free'},
  journal = {AEA Papers and Proceedings},
  year = {2018},
  volume = {108},
  pages = {38--42},
  doi = {10.1257/pandp.20181003},
  note = {CORE ARGUMENT: Challenges conventional framing of user data as capital created by corporations observing willing individuals. Argues this neglects users' roles in creating data, reduces user incentives, distributes data economy gains unequally, and stokes automation fears. Proposes treating data (at least partially) as labor to restore functioning market for user contributions. May run against interests of dominant data monopsonists who benefit from data being "free." Advocates for recognizing workers' data contributions as valuable labor deserving compensation. RELEVANCE: Provocative reconceptualization of data production as labor rather than byproduct. Platform and AI workers generate valuable training data but receive no compensation. Treating data as labor would fundamentally reshape platform economics and power relations. Raises fundamental questions about value creation, ownership, and compensation in digital economy. Essential for understanding how platform architectures extract value from worker data generation. POSITION: Economics of data (radical data markets proposal, labor perspective).},
  keywords = {data-as-labor, platform-economy, data-economics, compensation, Medium}
}

@article{delfanti2021humanly,
  author = {Delfanti, Alessandro and Frey, Bronwyn},
  title = {Humanly Extended Automation or the Future of Work Seen through Amazon Patents},
  journal = {Science, Technology, \& Human Values},
  year = {2021},
  volume = {46},
  number = {3},
  pages = {655--682},
  doi = {10.1177/0162243920943665},
  note = {CORE ARGUMENT: Systematic analysis of Amazon warehouse patents reveals that workers are not disappearing from warehouse floor but being reconfigured. Many patents portray machines that increase worker surveillance and work rhythms, while others incorporate workers' activities into machinery to rationalize labor process in form of "digital Taylorism." Patents show Amazon's vision of humanly extended automation where humans and machines are intimately integrated with humans performing tasks machines cannot yet automate. Shows automation as reshaping not eliminating human work. RELEVANCE: Patent analysis provides unique window into corporate automation strategies and future work visions. Humanly extended automation challenges simple automation/augmentation binary - humans and machines are deeply integrated. Digital Taylorism concept connects contemporary algorithmic management to historical scientific management, showing continuities. Surveillance and work intensification revealed through patents demonstrate autonomy costs of warehouse automation. Essential for understanding embodied, blue-collar algorithmic management. POSITION: Science and technology studies (patent analysis, warehouse automation, critical perspective).},
  keywords = {amazon, warehouse-automation, patents, digital-taylorism, surveillance, High}
}

@book{delfanti2021warehouse,
  author = {Delfanti, Alessandro},
  title = {The Warehouse: Workers and Robots at Amazon},
  publisher = {Pluto Press},
  year = {2021},
  address = {London},
  note = {CORE ARGUMENT: Examines how Amazon uses computer algorithms, pervasive surveillance systems, and managerial techniques to control massive labor force in warehouses. Workers are not being replaced by robots but rather reconfigured into human-machine assemblages where both are tightly managed by algorithmic systems. Amazon represents paradigmatic case of algorithmic management in physical workplaces, not just digital platforms. Combines ethnographic observation, patent analysis, and worker testimony to reveal warehouse labor conditions under algorithmic control. RELEVANCE: Essential case study of algorithmic management in blue-collar, embodied work contexts. Demonstrates algorithmic control extends far beyond white-collar and platform work to warehouse operations. Human-robot integration reveals how automation reshapes rather than eliminates human labor. Surveillance and pace intensification under algorithmic management raise fundamental questions about worker dignity and autonomy. Amazon's scale and influence make it paradigmatic for understanding future of algorithmic management. POSITION: Critical ethnography (warehouse labor, automation, algorithmic control).},
  keywords = {amazon, warehouse-work, robots, algorithmic-control, surveillance, Medium}
}

@article{destefano2016rise,
  author = {De Stefano, Valerio},
  title = {The Rise of the `Just-in-Time Workforce': On-Demand Work, Crowdwork, and Labor Protection in the Gig Economy},
  journal = {Comparative Labor Law \& Policy Journal},
  year = {2016},
  volume = {37},
  number = {3},
  pages = {471--504},
  note = {CORE ARGUMENT: Analyzes how gig economy including crowdwork and work-on-demand via apps has grown exponentially but labor rights impacts largely overlooked. Just-in-time workforce operates with minimal labor protections despite platform control over working conditions. Examines legal classification challenges and argues most gig workers should be considered employees under traditional labor law despite platform claims. Influential ILO working paper and journal article documenting early gig economy labor law challenges. RELEVANCE: Foundational legal analysis of employment classification issues in platform work. Just-in-time workforce concept captures how platforms achieve employer control without employer responsibilities. Labor protection gaps demonstrate how platform architectures can evade established legal frameworks. Essential for understanding regulatory challenges and debates over worker status. Influential for policy discussions about extending labor protections to platform workers. POSITION: Labor law analysis (employment classification, regulatory frameworks, comparative).},
  keywords = {gig-economy, labor-law, employment-classification, crowdwork, platform-work, High}
}

@article{collier2017labor,
  author = {Collier, Ruth Berins and Dubal, Veena B. and Carter, Christopher L.},
  title = {Labor Platforms and Gig Work: The Failure to Regulate},
  journal = {IRLE Working Paper},
  year = {2017},
  number = {106-17},
  institution = {UC Berkeley Institute for Research on Labor and Employment},
  url = {http://www.irle.berkeley.edu/files/2017/Labor-Platforms-and-Gig-Work.pdf},
  note = {CORE ARGUMENT: Provides conceptual and theoretical overview of rapidly growing labor platforms, examining how they represent both continuity and change in world of work and regulation. Situates platforms within decline of labor protections and rise of intermediated employment since 1970s. Analyzes political problem of labor regulation, focusing on Uber as easiest case for regulation due to high degree of control. Platforms exercise significant control over working conditions while avoiding employer classification and responsibilities. RELEVANCE: Important early analysis of platform work regulation challenges. Shows platforms as latest iteration of employer strategies to avoid labor law obligations while maintaining control. Political economy perspective reveals how platform power shapes regulatory debates. Uber focus demonstrates that even most controlled platforms successfully avoid employee classification. Essential for understanding legal and political dimensions of platform work. Foundational for regulatory debates. POSITION: Labor law and political economy (regulatory analysis, platform classification).},
  keywords = {platform-work, labor-regulation, employment-classification, uber, political-economy, Medium}
}

@book{prassl2018humans,
  author = {Prassl, Jeremias},
  title = {Humans as a Service: The Promise and Perils of Work in the Gig Economy},
  publisher = {Oxford University Press},
  year = {2018},
  address = {Oxford},
  isbn = {9780198797012},
  note = {CORE ARGUMENT: Argues that most gig economy work falls within scope of employment law despite platform claims. Platforms like Uber, Amazon Mechanical Turk, Deliveroo, and TaskRabbit present as marketplaces but act like traditional employers. Claims of "disruptive innovation" and "micro-entrepreneurship" obscure realities of precarious work under strict algorithmic surveillance. Examines how platforms' digital work intermediation profoundly impacts traditional employment relationship conceptions. Advocates for employment law to address these problems and level playing field. RELEVANCE: Influential legal analysis arguing for employee status for platform workers. "Humans as a Service" concept captures commodification of workers through platform architectures. Shows how algorithmic management enables employer control while avoiding employer responsibilities. Essential for understanding legal debates over platform worker classification. Makes normative case for extending labor protections based on functional reality of platform control. POSITION: Labor law (normative, pro-worker classification, comparative legal analysis).},
  keywords = {gig-economy, employment-law, platform-work, worker-classification, algorithmic-surveillance, Medium}
}

@article{ajunwa2020black,
  author = {Ajunwa, Ifeoma},
  title = {The `Black Box' at Work},
  journal = {Big Data \& Society},
  year = {2020},
  volume = {7},
  number = {2},
  doi = {10.1177/2053951720938093},
  note = {CORE ARGUMENT: Examines algorithmic hiring and workplace monitoring systems as "black boxes" lacking transparency and accountability. Digital discrimination affects millions of workers who often don't know they're being algorithmically assessed. Algorithmic opacity prevents workers from understanding, contesting, or correcting automated decisions about hiring, evaluation, and discipline. Advocates for auditing requirements and transparency safeguards to protect workers' civil rights in digital age. Influential work informing policy debates and Congressional testimony. RELEVANCE: Important legal and ethical analysis of workplace algorithmic transparency. Black box opacity directly undermines worker autonomy by preventing understanding of systems governing employment. Digital discrimination risks show how algorithmic systems can perpetuate and hide bias. Essential for understanding transparency and accountability requirements for workplace AI. Connects to broader debates about explainable AI and algorithmic fairness. POSITION: Law and technology ethics (workplace AI, transparency, anti-discrimination).},
  keywords = {workplace-surveillance, algorithmic-hiring, transparency, discrimination, black-box, High}
}

@article{ajunwa2021auditing,
  author = {Ajunwa, Ifeoma},
  title = {The Auditing Imperative for Automated Hiring},
  journal = {Harvard Journal of Law \& Technology},
  year = {2021},
  volume = {34},
  number = {2},
  pages = {621--695},
  note = {CORE ARGUMENT: Argues for mandatory auditing of automated hiring systems to prevent discrimination and protect workers' civil rights. Algorithmic hiring tools proliferate without adequate oversight or accountability. Without auditing requirements, employers can deploy discriminatory systems while claiming technological neutrality. Proposes comprehensive auditing framework including pre-deployment testing, ongoing monitoring, and independent third-party audits. Influential legal scholarship informing regulatory proposals. RELEVANCE: Develops practical governance framework for workplace AI systems. Auditing imperative addresses accountability gap in algorithmic management - systems deployed without verification they meet legal and ethical standards. Essential for thinking about institutional safeguards necessary to protect worker autonomy under algorithmic management. Connects to broader AI governance debates about testing, certification, and accountability. POSITION: Law and technology policy (algorithmic auditing, automated hiring, governance frameworks).},
  keywords = {algorithmic-hiring, auditing, discrimination, workplace-AI, governance, Medium}
}

@article{jobin2019global,
  author = {Jobin, Anna and Ienca, Marcello and Vayena, Effy},
  title = {The Global Landscape of AI Ethics Guidelines},
  journal = {Nature Machine Intelligence},
  year = {2019},
  volume = {1},
  pages = {389--399},
  doi = {10.1038/s42256-019-0088-2},
  note = {CORE ARGUMENT: Maps and analyzes 84 AI ethics guidelines from national/international organizations, companies, and institutes globally. Reveals convergence around five principles: transparency, justice and fairness, non-maleficence, responsibility, and privacy. But substantial divergence exists in how principles are interpreted, why they matter, what domains/actors they pertain to, and how to implement them. Notes global regions participate unequally in AI ethics debate - underrepresentation of Africa, South/Central America, and Central Asia. RELEVANCE: Comprehensive survey of global AI ethics landscape essential for understanding normative frameworks applicable to workplace AI. Convergence on transparency, fairness, and responsibility directly relevant to algorithmic management ethics. Divergence in interpretation and implementation highlights challenges of translating principles into practice. Geographic inequality in AI ethics discourse suggests Global South perspectives underrepresented. Essential reference for normative analysis of workplace AI systems. POSITION: AI ethics empirical mapping (global survey, principles-based approach).},
  keywords = {AI-ethics, ethical-principles, transparency, fairness, global-survey, Medium}
}

@inproceedings{bender2021stochastic,
  author = {Bender, Emily M. and Gebru, Timnit and McMillan-Major, Angelina and Shmitchell, Shmargaret},
  title = {On the Dangers of Stochastic Parrots: Can Language Models Be Too Big?},
  booktitle = {Proceedings of the 2021 ACM Conference on Fairness, Accountability, and Transparency (FAccT 2021)},
  year = {2021},
  pages = {610--623},
  doi = {10.1145/3442188.3445922},
  note = {CORE ARGUMENT: Examines risks of large language models including environmental costs, bias amplification, and language inequality. "Stochastic parrot" metaphor frames LLMs as probabilistically linking words without meaning or understanding. Models can reproduce/amplify unwanted biases, propagating stereotypes and problematic associations. Primarily serve dominant languages and privileged communities. Raises fundamental questions about what counts as intelligence and whether scale alone produces understanding. Controversial paper - Gebru's forced departure from Google followed disputes over it. RELEVANCE: Important critical perspective on AI systems increasingly deployed in workplace contexts for hiring, evaluation, communication. Challenges anthropomorphization of AI systems - they statistically mimic without understanding. Bias risks particularly salient for workplace applications where discrimination concerns are paramount. Essential for understanding limitations and risks of AI systems marketed as intelligent assistants. Reminds that AI systems can undermine rather than support worker autonomy and dignity. POSITION: Critical AI research (language models, ethics, environmental/social costs).},
  keywords = {large-language-models, AI-bias, stochastic-parrots, AI-ethics, critical-AI, Medium}
}

@book{pasquale2015black,
  author = {Pasquale, Frank},
  title = {The Black Box Society: The Secret Algorithms that Control Money and Information},
  publisher = {Harvard University Press},
  year = {2015},
  address = {Cambridge, MA},
  note = {CORE ARGUMENT: Examines how opaque algorithms increasingly control decisions in finance, reputation, and search with limited transparency or accountability. "Black box" algorithms can make or ruin reputations, determine business success, or devastate economies. Digital scoring processes daily activities as signals for rewards or penalties by hidden algorithms. Tendency to trust algorithms problematic when they may embed prior discrimination, reinforcing and hiding bias. Argues for transparency and accountability requirements to address runaway data and black box decision-making. RELEVANCE: Foundational critique of algorithmic opacity across sectors including workplace. Black box metaphor influential for understanding transparency challenges. Trust in algorithmic systems without ability to audit them creates accountability gap. Discrimination embedding and amplification particularly concerning for workplace applications (hiring, evaluation, promotion). Essential for understanding why algorithmic transparency matters for worker autonomy and fairness. Influential legal scholarship shaping policy debates. POSITION: Law and political economy (algorithmic accountability, transparency, critical analysis).},
  keywords = {algorithmic-opacity, black-box, transparency, accountability, discrimination, High}
}

@book{pasquale2020new,
  author = {Pasquale, Frank},
  title = {New Laws of Robotics: Defending Human Expertise in the Age of AI},
  publisher = {Harvard University Press},
  year = {2020},
  address = {Cambridge, MA},
  note = {CORE ARGUMENT: Proposes four new laws to guide AI and robotics: (1) robots/AI should complement professionals not replace them, (2) should not counterfeit humanity, (3) should not intensify zero-sum arms races, (4) must respect human judgment in critical roles. Examines AI across healthcare, education, media, law enforcement, recruitment to show thousands of small steps toward automation. Argues policymakers must not allow corporations or engineers to answer automation questions alone - need democratization of technology governance. Advocates for human expertise preservation and worker partnership with AI rather than replacement. RELEVANCE: Normative framework for AI governance emphasizing human autonomy and expertise preservation. Complementarity principle directly relevant to workplace AI - systems should support not supplant human judgment. Challenges automation orthodoxy by arguing for democratic technology governance. Essential for thinking about alternative AI development paths that preserve worker agency and dignity. Shows automation as political choice not technological inevitability. POSITION: Technology law and ethics (normative framework, human expertise defense, governance).},
  keywords = {AI-governance, robotics, human-expertise, complementarity, normative-framework, Medium}
}

@book{eubanks2018automating,
  author = {Eubanks, Virginia},
  title = {Automating Inequality: How High-Tech Tools Profile, Police, and Punish the Poor},
  publisher = {St. Martin's Press},
  year = {2018},
  address = {New York},
  note = {CORE ARGUMENT: Investigates data mining, policy algorithms, and predictive risk models' impacts on poor and working-class people through three case studies: Indiana welfare system denial of one million applications due to computer errors, Los Angeles homeless services algorithm prioritizing inadequate housing pool, and Pittsburgh child welfare predictive model. Argues automated systems separate people from resources, classify and criminalize, invade privacy - problems that will eventually affect everyone. Automated systems entrench inequality by design and undermine welfare. Won 2019 Lillian Smith Book Award and 2018 McGannon Center Book Prize. RELEVANCE: Essential critical analysis of algorithmic decision-making in public services with clear parallels to workplace contexts. Shows how algorithmic systems can systematically disadvantage vulnerable populations. Mistake-as-noncompliance logic in Indiana case reveals how algorithmic rigidity eliminates human judgment and discretion. Demonstrates automation is not neutral but embeds policy and power choices. Cautionary tale for workplace algorithmic management - same dynamics of bias, opacity, and dehumanization can occur. POSITION: Critical technology studies (algorithmic inequality, public systems, ethnographic case studies).},
  keywords = {algorithmic-inequality, automated-decision-systems, public-services, bias, Medium}
}

@book{autor2022work,
  author = {Autor, David H. and Mindell, David A. and Reynolds, Elisabeth B.},
  title = {The Work of the Future: Building Better Jobs in an Age of Intelligent Machines},
  publisher = {MIT Press},
  year = {2022},
  address = {Cambridge, MA},
  note = {CORE ARGUMENT: Based on MIT Task Force with 20 faculty and 20 graduate students, argues technology is neither problem nor solution to labor market challenges. "Future of AI is future of work" - need to understand AI capabilities and limitations. Recommends investing in skills and training (work-based and hybrid learning), improving job quality (modernizing unemployment insurance and labor laws), and shaping innovation (increasing R\&D spending, rebalancing capital-labor taxes). Skills programs emphasizing continuous learning empower workers in evolving workplace. Challenges both techno-optimism and techno-pessimism. RELEVANCE: Influential institutional report shaping policy debates about AI and work. Emphasizes that technology outcomes depend on social choices about deployment, governance, and investment. Skills and training focus shows importance of worker capabilities for maintaining autonomy under technological change. Job quality emphasis demonstrates that automation impacts depend on institutional contexts and policy choices. Essential for understanding mainstream policy proposals for managing AI workplace transitions. POSITION: Institutional policy research (MIT Task Force, balanced technology perspective, policy recommendations).},
  keywords = {future-of-work, AI-policy, skills-training, job-quality, institutional-report, Medium}
}

@article{krzywdzinski2021automation,
  author = {Krzywdzinski, Martin},
  title = {Automation, Digitalization, and Changes in Occupational Structures in the Automobile Industry in Germany, Japan, and the United States: A Brief History from the Early 1990s until 2018},
  journal = {Industrial and Corporate Change},
  year = {2021},
  volume = {30},
  number = {3},
  pages = {499--535},
  doi = {10.1093/icc/dtab019},
  note = {CORE ARGUMENT: Examines evolution of automation and digitalization in automotive industry 1990s-2018 through qualitative press analysis and quantitative robot stock data (International Federation of Robotics). Analyzes employment structure changes using occupational statistics from US, Germany, and Japan. Questions automation-related employment threat perception, especially for production employment. Highlights differences in automation/digitalization approaches across countries and different paths of employment structure change. Shows automation does not uniformly eliminate jobs but reshapes occupational composition. RELEVANCE: Important empirical study challenging simplistic automation displacement narratives. Shows automation impacts vary significantly by national context, industry strategy, and institutional framework. Production employment persistence despite heavy automation suggests human labor adapts rather than disappears. Essential for understanding that automation effects are mediated by social choices not determined by technology. Cross-national comparison reveals multiple possible automation trajectories. POSITION: Industrial sociology (comparative, historical, automotive industry focus).},
  keywords = {automation, automotive-industry, comparative-analysis, employment-structures, Medium}
}

@report{krzywdzinski2020varieties,
  author = {Krzywdzinski, Martin and Gerber, Christine},
  title = {Varieties of Platform Work: Platforms and Social Inequality in Germany and the United States},
  institution = {Weizenbaum Institute},
  year = {2020},
  number = {Weizenbaum Series 7},
  address = {Berlin},
  url = {https://www.weizenbaum-institut.de/media/Publikationen/Weizenbaum_Series/Weizenbaum_Series_7_Krzywdzinski_210412.pdf},
  note = {CORE ARGUMENT: Examines how platform economy reproduces, reduces, or increases social inequalities through case studies of 15 crowdwork platforms in US and Germany plus online survey of crowdworkers. Platform economy criticized for exacerbating inequalities but actual patterns vary significantly across platform types and national contexts. Different platform designs and regulatory environments create distinct inequality patterns. Comparative methodology reveals multiple forms of platform work with different implications for worker well-being and social stratification. RELEVANCE: Important empirical study showing platform work heterogeneity challenges generalizations about platform impacts. Inequality focus demonstrates that platform effects depend on design choices and regulatory contexts, not technological determinism. Germany-US comparison shows how institutional frameworks shape platform work quality. Essential for understanding that platforms can be designed and regulated differently with varied inequality consequences. POSITION: Comparative platform work research (inequality focus, institutional analysis).},
  keywords = {platform-work, social-inequality, comparative-analysis, crowdwork, institutions, Low}
}

@article{kassi2018online,
  author = {K{\"a}ssi, Otto and Lehdonvirta, Vili},
  title = {Online Labour Index: Measuring the Online Gig Economy for Policy and Research},
  journal = {Technological Forecasting and Social Change},
  year = {2018},
  volume = {137},
  pages = {241--248},
  doi = {10.1016/j.techfore.2018.07.056},
  note = {CORE ARGUMENT: Develops Online Labour Index (OLI) as experimental economic indicator approximating conventional labor market statistic of new open vacancies. Tracks projects and tasks posted on major online gig platforms in near-real time, measuring online labor utilization across countries and occupations. Provides data infrastructure for studying global online labor markets and monitoring platform economy growth. OLI enables researchers and policymakers to track online gig work trends quantitatively. Online Labour Observatory ceased data collection summer 2024. RELEVANCE: Important methodological contribution providing quantitative data on online gig economy previously difficult to measure. Real-time tracking enables analysis of platform labor market dynamics and trends. Cross-country and cross-occupation data reveals global patterns in online work distribution. Essential infrastructure for empirical platform work research. Demonstrates feasibility of monitoring platform economy despite platform opacity. Methodological innovation for studying digital labor markets. POSITION: Platform work measurement (quantitative methodology, labor market indicators).},
  keywords = {online-labor-index, platform-work-measurement, gig-economy-data, methodology, Low}
}

@article{wood2019networked,
  author = {Wood, Alex J. and Graham, Mark and Lehdonvirta, Vili and Hjorth, Isis},
  title = {Networked but Commodified: The (Dis)embeddedness of Digital Labour in the Gig Economy},
  journal = {Sociology},
  year = {2019},
  volume = {53},
  number = {5},
  pages = {931--950},
  doi = {10.1177/0038038519828906},
  note = {CORE ARGUMENT: Examines embeddedness and disembeddedness of digital labor in gig economy. Platform workers networked globally but their labor is commodified and disembedded from social relations and protections. Platforms facilitate market transactions while stripping away social embedding that traditionally provided worker security and dignity. Workers remain vulnerable to demand fluctuations with limited social protection despite digital connectivity. Both platform types (online freelancing and local on-demand) institutionalize precarity through commodification despite apparent networking. RELEVANCE: Important theoretical contribution applying Polanyian embeddedness concept to platform work. Shows digital connectivity does not necessarily translate into social embeddedness or protection. Commodification of labor through platform markets undermines traditional social relations that provided worker security. Essential for understanding how platform architectures can increase economic precarity despite technological connectivity. Challenges optimistic narratives about digital platforms enabling community and solidarity. POSITION: Economic sociology (Polanyian framework, embeddedness, platform labor).},
  keywords = {platform-work, embeddedness, commodification, precarity, digital-labor, Medium}
}

@article{sutherland2019algorithmic,
  author = {Sutherland, Will and Jarrahi, Mohammad Hossein},
  title = {Algorithmic Management and Algorithmic Competencies: Understanding and Appropriating Algorithms in Gig Work},
  booktitle = {iConference 2019 Proceedings},
  year = {2019},
  doi = {10.21900/iconf.2019.103387},
  note = {CORE ARGUMENT: Empirical study of Upwork users demonstrates workers are not passive recipients of algorithmic management. Workers develop "algorithmic competencies" - literacy for understanding algorithms despite information asymmetries. Three strategies identified: sensemaking (understanding platform logic), circumventing (working around constraints), and manipulating (gaming systems to retain autonomy). Data-driven algorithms enable platforms to automatically manage transactions but workers develop knowledge to maintain professional autonomy despite platform control. RELEVANCE: Important demonstration that worker agency persists under algorithmic management through active learning and adaptation. Algorithmic competencies concept captures how workers navigate opacity and constraints. Shows autonomy as actively achieved through effort and skill, not passively given. Circumvention and manipulation strategies reveal resistance embedded in everyday platform work practices. Essential for understanding worker agency as dynamic capability developed in response to algorithmic control. POSITION: Information systems research (worker strategies, algorithmic literacy, Upwork platform).},
  keywords = {algorithmic-competencies, worker-agency, upwork, gig-work, resistance-strategies, Medium}
}

@article{newlands2018collective,
  author = {Newlands, Gemma},
  title = {Collective Action and Provider Classification in the Sharing Economy},
  journal = {New Technology, Work and Employment},
  year = {2018},
  volume = {33},
  number = {3},
  pages = {250--267},
  doi = {10.1111/ntwe.12119},
  note = {CORE ARGUMENT: Examines how sharing economy providers make voices heard collectively despite conditions favoring consumers and platforms over labor. Cluster analysis on representative data from 12 European countries identifies five distinct types of labor-activists among sharing economy workers. Platform workers develop collective action strategies adapted to platform contexts despite geographic dispersion and individual contractor status. Demonstrates collective organizing possible even under atomizing platform architectures and unfavorable legal classifications. RELEVANCE: Important early study of platform worker organizing and collective action. Five labor-activist types reveal diversity in worker political engagement and strategies. Shows workers finding ways to organize despite platform design that individualizes labor relations. Essential for understanding worker agency beyond individual navigation to include collective resistance. Challenges view that platform atomization makes organizing impossible. European scope reveals cross-national patterns in platform worker mobilization. POSITION: Labor sociology (collective action, sharing economy, European comparative).},
  keywords = {collective-action, platform-work, sharing-economy, labor-organizing, worker-classification, Medium}
}

@book{cant2019riding,
  author = {Cant, Callum},
  title = {Riding for Deliveroo: Resistance in the New Economy},
  publisher = {Polity},
  year = {2019},
  address = {Cambridge, UK},
  note = {CORE ARGUMENT: Based on eight months working as Deliveroo rider and participating in organizing/strikes in Brighton, examines labor processes and resistance dynamics of food delivery couriers. Uses Italian Workerist tradition and workers' inquiry method, assuming workers' point of view in analyzing labor process. Reveals transnational network of encrypted chats and informal groups birthing wave of strikes and protests. Challenges narrative of atomized, helpless platform workers, showing workers "tearing up rulebook and taking back control" through grassroots organizing. RELEVANCE: Important first-person account of platform worker organizing and resistance from inside. Workers' inquiry methodology provides grounded perspective on resistance strategies and consciousness. Demonstrates collective action emerging from workers' shared experiences despite platform fragmentation. Transnational solidarity networks reveal new forms of labor organizing adapted to platform contexts. Essential for understanding worker agency and resistance as collective practice, not just individual navigation. Shows platform workers as political actors, not passive victims. POSITION: Critical ethnography (workers' inquiry method, Workerist tradition, food delivery).},
  keywords = {deliveroo, worker-resistance, food-delivery, workers-inquiry, organizing, strikes, High}
}

@article{bucher2017algorithmic,
  author = {Bucher, Taina},
  title = {The Algorithmic Imaginary: Exploring the Ordinary Affects of Facebook Algorithms},
  journal = {Information, Communication \& Society},
  year = {2017},
  volume = {20},
  number = {1},
  pages = {30--44},
  doi = {10.1080/1369118X.2016.1154086},
  note = {CORE ARGUMENT: Examines ordinary users' "algorithmic imaginaries" - ways of thinking about what algorithms are, what they should be, and how they function. Based on tweets and interviews with 25 Facebook users about News Feed algorithm. Argues algorithmic imaginary plays generative role in molding Facebook algorithm itself, not just reflecting it. Explores situations where people become aware of algorithms, how they make sense of hidden/invisible systems, and whether awareness affects platform use. Shows users developing folk theories about algorithmic operation despite opacity. RELEVANCE: Important concept for understanding how workers make sense of opaque algorithmic management systems. Algorithmic imaginaries shape how people respond to and work with algorithmic systems even without technical understanding. Folk theories and sense-making crucial for worker agency under opacity. Shows awareness and imagination as forms of knowledge even when algorithmic logic remains hidden. Influential for platform governance and algorithmic awareness research. Essential for understanding subjective experience of algorithmic systems. POSITION: Media and communication studies (user perspectives, algorithmic awareness, Facebook).},
  keywords = {algorithmic-imaginary, algorithmic-awareness, sense-making, facebook, user-experience, Low}
}

@article{shapiro2020dynamic,
  author = {Shapiro, Aaron},
  title = {Dynamic Exploits: Calculative Asymmetries in the On-Demand Economy},
  journal = {New Technology, Work and Employment},
  year = {2020},
  volume = {35},
  number = {2},
  pages = {162--177},
  doi = {10.1111/ntwe.12160},
  note = {CORE ARGUMENT: Analyzes how on-demand firms secure market power by cultivating calculative asymmetries between platforms and labor. Dynamic/surge pricing exemplifies calculative technique that appears as neutral market mechanism but enables platform manipulation of worker decision-making. Platforms develop covert strategies to influence workers through dynamic pricing and wages, providing insights into exploitative calculus. Calculative asymmetries undermine worker autonomy by making informed decision-making impossible. Earlier 2018 report expanded into 2020 journal article. RELEVANCE: Important analysis of how platform calculation and pricing mechanisms function as control tools. Calculative asymmetries concept captures epistemic power imbalances - platforms know and calculate what workers cannot. Dynamic pricing reveals how apparently neutral economic mechanisms can be manipulative when information is asymmetric. Essential for understanding platform control as operating through economic calculation not just direct commands. Shows autonomy requires epistemic parity, not just formal choice. POSITION: Critical technology studies (political economy, platform calculation, on-demand economy).},
  keywords = {dynamic-pricing, calculative-asymmetries, on-demand-economy, platform-power, information-asymmetry, Medium}
}

@book{ravenelle2019hustle,
  author = {Ravenelle, Alexandrea J.},
  title = {Hustle and Gig: Struggling and Surviving in the Sharing Economy},
  publisher = {University of California Press},
  year = {2019},
  address = {Oakland, CA},
  note = {CORE ARGUMENT: Ethnographic study of nearly 80 Airbnb hosts, Uber drivers, TaskRabbit assistants, and Kitchensurfing chefs examines contradictions between gig economy promises and lived experiences. Identifies three worker types: Success Stories (created desired life), Strugglers (can't make ends meet), and Strivers (stable jobs using gig for extra cash). Exposes gig economy as millennial's version of minimum-wage precarious work, examining how lofty promises contrast with reality of rolling back workplace protections. Draws on eight months fieldwork as Deliveroo rider participating in organizing. RELEVANCE: Important typology showing variation in platform worker experiences and outcomes. Not all platform workers equally disadvantaged - experiences mediated by existing resources and goals. Success/Struggler/Striver framework reveals how inequality shapes gig economy participation and consequences. Demonstrates gig economy reproduces and intensifies existing stratification. Essential for understanding heterogeneous impacts of platform work. Challenges both optimistic and uniformly pessimistic narratives. POSITION: Economic sociology (ethnography, worker typology, sharing economy).},
  keywords = {sharing-economy, gig-work, ethnography, worker-experiences, inequality, Medium}
}

@article{dubal2023algorithmic,
  author = {Dubal, Veena B.},
  title = {On Algorithmic Wage Discrimination},
  journal = {Columbia Law Review},
  year = {2023},
  volume = {123},
  number = {7},
  note = {CORE ARGUMENT: Documents how platforms like Uber use granular worker data to implement "algorithmic wage discrimination" - unpredictable, variable, personalized hourly pay based on individual worker characteristics and behaviors. Platforms collect data on where workers live, when/where they work, income goals, and job acceptance patterns. Algorithmic wage-setting functions like consumer price discrimination, charging each person what platforms determine they'll accept. From workers' perspectives, creates unpredictable pay that workers describe as "gamification" and "gambling." UC Hastings law professor's groundbreaking research on algorithmic discrimination. RELEVANCE: Reveals sophisticated platform techniques for individualizing pay and undermining collective bargaining power. Algorithmic wage discrimination shows how platforms use data and algorithms to maximize extraction from workers. Unpredictability and personalization prevent workers from making informed comparisons or organizing around shared interests. Essential for understanding how algorithmic management enables new forms of discrimination and exploitation. Challenges wage transparency and pay equity fundamentals. POSITION: Critical labor law (algorithmic discrimination, platform wages, empirical legal studies).},
  keywords = {algorithmic-wage-discrimination, uber, platform-wages, data-discrimination, gamification, High}
}

@incollection{ticona2018trusted,
  author = {Ticona, Julia and Mateescu, Alexandra},
  title = {Trusted Strangers: Carework Platforms' Cultural Entrepreneurship in the On-Demand Economy},
  journal = {New Media \& Society},
  year = {2018},
  volume = {20},
  number = {11},
  pages = {4384--4404},
  doi = {10.1177/1461444818773727},
  note = {CORE ARGUMENT: Examines care and domestic work platforms often excluded from platform work conversations dominated by male-dominated ridehail sector. Workers with marginalized identities (people of color, undocumented workers, largely women) report inequitable conditions from rating systems and other platform features. Digital literacy becomes determining factor in securing work, often above actual domestic care skills. Marketplace platforms make digital skills prerequisite for work access. Co-authored with Alex Rosenblat for Data & Society "Beyond Disruption" report. RELEVANCE: Essential inclusion of feminized, racialized platform work often overlooked in algorithmic management research. Demonstrates how algorithmic systems can amplify existing inequalities based on race, gender, immigration status. Digital literacy requirements add barrier beyond job skills, excluding some workers. Rating systems function differently and more punitively for marginalized workers. Crucial for intersectional analysis of algorithmic management showing differential impacts. POSITION: Critical platform work research (care work focus, intersectional analysis, gender/race).},
  keywords = {care-work, domestic-work, platform-work, intersectionality, rating-systems, gender-race, Medium}
}

@report{mateescu2019algorithmic,
  author = {Mateescu, Alexandra and Nguyen, Aiha},
  title = {Algorithmic Management in the Workplace},
  institution = {Data \& Society},
  year = {2019},
  address = {New York},
  type = {Explainer},
  url = {https://datasociety.net/library/explainer-algorithmic-management-in-the-workplace/},
  note = {CORE ARGUMENT: Defines algorithmic management as "diverse set of technology tools and techniques that structure work conditions and remotely manage workforces" through data collection and worker surveillance enabling automated decision-making. Maps AI use in workplace management across gig economy, domestic work, and retail sectors. Identifies four broad challenge areas: surveillance and control, transparency, bias and discrimination, and accountability. Provides accessible overview for policymakers and public. Co-released with explainer on Workplace Monitoring & Surveillance. RELEVANCE: Influential early synthesis defining algorithmic management for policy audiences. Four challenge areas framework widely adopted in subsequent research and advocacy. Cross-sector mapping demonstrates algorithmic management as pervasive phenomenon, not gig economy-specific. Accessibility and policy focus helped shape regulatory discussions. Essential introduction to algorithmic management issues. Data & Society institutional authority gave framework significant reach and influence. POSITION: Policy research and advocacy (definitional framework, cross-sector mapping).},
  keywords = {algorithmic-management, data-society, policy-explainer, surveillance, transparency, accountability, Medium}
}

@article{ravenelle2024beyond,
  author = {Wood, Alex J. and Martindale, Nicholas and Burchell, Brendan J.},
  title = {Beyond the `Gig Economy': Towards Variable Experiences of Job Quality in Platform Work},
  journal = {Work, Employment and Society},
  year = {2025},
  doi = {10.1177/09500170251336947},
  note = {CORE ARGUMENT: Recent 2025 publication examining job quality variation in platform work beyond simple gig economy categorization. Argues for more nuanced understanding of how platform work quality varies by platform type, sector, worker characteristics, and regulatory context. Challenges binary good/bad characterizations of platform work by documenting significant heterogeneity. Builds on authors' earlier global gig economy research to develop more sophisticated framework for analyzing platform work quality dimensions. RELEVANCE: Important recent contribution moving beyond early platform work characterizations to more complex analysis. Demonstrates that platform work cannot be understood as monolithic category. Job quality variation shows platform design and regulatory choices matter for worker experiences and outcomes. Essential for understanding that platform work quality is not technologically determined but socially shaped. Recent publication shows continuing evolution of platform work research sophistication. POSITION: Platform work research (job quality focus, heterogeneity, recent empirical).},
  keywords = {platform-work, job-quality, heterogeneity, gig-economy, recent-research, Low}
}
