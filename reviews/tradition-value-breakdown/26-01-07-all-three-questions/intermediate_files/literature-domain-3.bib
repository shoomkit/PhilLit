@book{horkheimer2020dialectic,
  author = {Horkheimer, Max and Adorno, Theodor W.},
  title = {Dialectic of Enlightenment},
  publisher = {Stanford University Press},
  year = {2020},
  doi = {10.1515/9780804788090},
  note = {
  CORE ARGUMENT: Horkheimer and Adorno argue that Enlightenment reason, in
  its quest to dominate nature and liberate humanity from myth and
  tradition, paradoxically generates new forms of domination and
  reification. Instrumental rationality—reason reduced to mere calculation—
  becomes a totalizing force that perpetuates the very barbarism the
  Enlightenment sought to overcome.

  RELEVANCE: This foundational critical theory text complicates simple
  Enlightenment critiques of tradition by revealing how rational critique
  itself can become oppressive. It is essential for understanding how
  traditions might degenerate not through irrationality but through the
  rationalization process itself, and for analyzing the dialectical
  relationship between tradition, reason, and domination.

  POSITION: Frankfurt School critical theory critique of instrumental
  reason and the internal contradictions of Enlightenment rationality.
  },
  keywords = {critical-theory, frankfurt-school, instrumental-reason,
             dialectic-enlightenment, High}
}

